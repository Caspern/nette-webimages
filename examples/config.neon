extensions:
	webimages: DotBlue\WebImages\Extension

webimages:
	providers:
		- DefaultImageProvider(%wwwDir%)
		- FakeImageProvider

	routes:
		- 'images/<id>-<width>x<height>.jpg'
		- mask: 'images/<id>-<width>x<height>.png'
		  format: png

	rules:
		- [width: 300, height: 200]
		- [width: 50, height: 30]
